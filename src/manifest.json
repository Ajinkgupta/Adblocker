{
	"name": "AdBlocker",
	"version": "1.0",
	"description": "Blocks ads and tracking scripts by Ajink Gupta",
	"manifest_version": 2,
	"permissions": [
		"webRequest",
		"webRequestBlocking",
		"notifications",
        "<all_urls>"
	],
	"background": {
		"scripts": ["background.js"]
		 
	},
	"browser_action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icons/logo.png",
        "48": "icons/logo.png",
        "128": "icons/logo.png"
    },
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"


}
